					*(O3 + 30 * Os) = * I3; I3 += Is;
					*(O4 + 30 * Os) = * I4; I4 += Is;
					*(O5 + 30 * Os) = * I5; I5 += Is;
					*(O6 + 30 * Os) = * I6; I6 += Is;
					*(O7 + 30 * Os) = * I7; I7 += Is;
					*(O0 + 31 * Os) = * I0; I0 += Is;
					*(O1 + 31 * Os) = * I1; I1 += Is;
					*(O2 + 31 * Os) = * I2; I2 += Is;
					*(O3 + 31 * Os) = * I3; I3 += Is;
					*(O4 + 31 * Os) = * I4; I4 += Is;
					*(O5 + 31 * Os) = * I5; I5 += Is;
					*(O6 + 31 * Os) = * I6; I6 += Is;
					*(O7 + 31 * Os) = * I7; I7 += Is;
					O0 += OS;
					O1 += OS;
					O2 += OS;
					O3 += OS;
					O4 += OS;
					O5 += OS;
					O6 += OS;
					O7 += OS;
				}
				break;
			}
		}
	}
	i = n & 31;
	if (i) {
		replicate_8x16(O0, i, I0, oS, iS, Os, Is); 
	}
}

