template <class T, class U>
static inline void replicate_4x1 (T* O0, 
																	volatile const U n,
																	T* I0 = 0, 
																	volatile const U oS = 1,
																	volatile const U iS = 0,
																	volatile const U Os = 1,
																	volatile const U Is = 0) {

	if (O0 == I0) { 
		return;
	}
	T* O1 = O0 + oS * 1;
	T* O2 = O0 + oS * 2;
	T* O3 = O0 + oS * 3;
	T* I1 = I0 + iS * 1;
	T* I2 = I0 + iS * 2;
	T* I3 = I0 + iS * 3;

	register T o0, o1, o2, o3;
	register U i = n;
  
	if (I0 == 0) { 
		if (Os == 1) {
			for (; i; i--) {
				*(O0 +  0) = 0.; 
				*(O1 +  0) = 0.; 
				*(O2 +  0) = 0.; 
				*(O3 +  0) = 0.; 
				O0 ++;
				O1 ++;
				O2 ++;
				O3 ++;
			}
		}
		else {
			for (; i; i--) {
				*(O0 +  Os * 0) = 0.; 
				*(O1 +  Os * 0) = 0.; 
				*(O2 +  Os * 0) = 0.; 
				*(O3 +  Os * 0) = 0.; 
				O0 += Os * 1;
				O1 += Os * 1;
				O2 += Os * 1;
				O3 += Os * 1;
			}
		}
	}
	else if (Os == 1) {
		switch (Is) {
		  case 0: {
				o0 = *I0;
				o1 = *I1;
				o2 = *I2;
				o3 = *I3;
				for (; i; i--) {
					*(O0 +  0) = o0; 
					*(O1 +  0) = o1; 
					*(O2 +  0) = o2; 
					*(O3 +  0) = o3; 
					O0 ++;
					O1 ++;
					O2 ++;
					O3 ++;
				}
				break;
			}
			case 1: {
				for (; i; i--) {
					*(O0 +  0) = *(I0 +  0); 
					*(O1 +  0) = *(I1 +  0); 
					*(O2 +  0) = *(I2 +  0); 
					*(O3 +  0) = *(I3 +  0); 
					I0 ++;
					O0 ++;
					I1 ++;
					O1 ++;
					I2 ++;
					O2 ++;
					I3 ++;
					O3 ++;
				}
				break;
			}
			default: {
				for (; i; i--) {
					*(O0 +  0) = * I0; I0 += Is;
					*(O1 +  0) = * I1; I1 += Is;
					*(O2 +  0) = * I2; I2 += Is;
					*(O3 +  0) = * I3; I3 += Is;
					O0 ++;
					O1 ++;
					O2 ++;
					O3 ++;
				}
				break;
			}
		}
	}
	else {
		switch (Is) {
		  case 0: {
				o0 = *I0;
				o1 = *I1;
				o2 = *I2;
				o3 = *I3;
				for (; i; i--) {
					*(O0 +  0 * Os) = o0; 
					*(O1 +  0 * Os) = o1; 
					*(O2 +  0 * Os) = o2; 
					*(O3 +  0 * Os) = o3; 
					O0 += 1 * Os;
					O1 += 1 * Os;
					O2 += 2 * Os;
					O3 += 3 * Os;
				}
				break;
			}
			case 1: {
				for (; i; i--) {
					*(O0 +  0 * Os) = *(I0 +  0);
					*(O1 +  0 * Os) = *(I1 +  0);
					*(O2 +  0 * Os) = *(I2 +  0);
					*(O3 +  0 * Os) = *(I3 +  0);
					I0 ++;
					O0 += 1 * Os;
					I1 ++;
					O1 += 1 * Os;
					I2 ++;
					O2 += 1 * Os;
					I3 ++;
					O3 += 1 * Os;
				}
				break;
			}
			default: {
				for (; i; i--) {
					*(O0 +  0 * Os) = * I0; I0 += Is;
					*(O1 +  0 * Os) = * I1; I1 += Is;
					*(O2 +  0 * Os) = * I2; I2 += Is;
					*(O3 +  0 * Os) = * I3; I3 += Is;
					O0 += 1 * Os;
					O1 += 1 * Os;
					O2 += 1 * Os;
					O3 += 1 * Os;
				}
				break;
			}
		}
	}
}

